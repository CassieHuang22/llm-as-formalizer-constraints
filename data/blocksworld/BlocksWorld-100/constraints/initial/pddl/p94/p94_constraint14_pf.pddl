(define (problem blocksworld-p94)
;; CONSTRAINT You start with the blocks in 3 stacks. The blocks in one tower are every third letter and the other tower does not contain every third letter but its index is odd, the final tower contains blocks that are not every third letter, but the letter index is even. The towers are stacked in alphabetical order with the lowest index letter on the table.
  (:domain blocksworld)
  (:objects a b c d e f g h i j )
  (:init 
;; BEGIN EDIT
    (on-table c)
    (on f c)
    (on i f)
    (clear i)
    (on-table a)
    (on e a)
    (on g e)
    (clear g)
    (on-table b)
    (on d b)
    (on h d)
    (on j h)
    (clear j)
;; END EDIT
    (arm-empty)
  )
  (:goal (and 
    (on-table a)
    (on b a)
    (on c b)
    (on-table d)
    (on e d)
    (on f e)
    (on g f)
    (on-table h)
    (on i h)
    (on j i)
  ))
)