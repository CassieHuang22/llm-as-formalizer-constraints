(define (problem blocksworld-p81)
;; CONSTRAINT You start with all blocks in a single tower in numerical order, with lowest number block on the table and the highest number block on top.
  (:domain blocksworld)
  (:objects block1 block2 block3 block4 block5 block6 block7 block8 block9 block10 block11 )
  (:init 
;; BEGIN EDIT
    (on-table block1)
    (on block2 block1)
    (on block3 block2)
    (on block4 block3)
    (on block5 block4)
    (on block6 block5)
    (on block7 block6)
    (on block8 block7)
    (on block9 block8)
    (on block10 block9)
    (on block11 block10)
    (clear block11)
;; END EDIT
    (arm-empty)
  )
  (:goal (and 
    (on-table block2)
    (on block5 block2)
    (on-table block8)
    (on-table block11)
    (on block9 block11)
    (on-table block4)
    (on-table block1)
    (on block10 block1)
    (on-table block3)
    (on-table block6)
    (on block7 block6)
  ))
)