(define (problem blocksworld-p35)
;; CONSTRAINT For every second move, you must move a block with a number that is a multiple of 5.
  (:domain blocksworld)
  (:objects block1 block2 block3 block4 block5 block6 block7 block8 block9 block10 block11 block12 block13 block14 )
  (:init 
    (on-table block13)
    (clear block13)
    (on-table block5)
    (clear block5)
    (on-table block11)
    (clear block11)
    (on-table block8)
    (on block14 block8)
    (clear block14)
    (on-table block2)
    (clear block2)
    (on-table block7)
    (clear block7)
    (on-table block6)
    (clear block6)
    (on-table block12)
    (clear block12)
    (on-table block3)
    (clear block3)
    (on-table block9)
    (clear block9)
    (on-table block4)
    (clear block4)
    (on-table block1)
    (clear block1)
    (on-table block10)
    (clear block10)
    (arm-empty)
;; BEGIN ADD
    (mult-5 block5) (mult-5 block10)
    (not-mult-5 block1) (not-mult-5 block2) (not-mult-5 block3) (not-mult-5 block4) (not-mult-5 block6) (not-mult-5 block7) (not-mult-5 block8) (not-mult-5 block9) (not-mult-5 block11) (not-mult-5 block12) (not-mult-5 block13) (not-mult-5 block14) 
    (last-none)
;; END ADD
  )
  (:goal (and 
    (on-table block4)
    (on-table block6)
    (on-table block14)
    (on-table block8)
    (on-table block9)
    (on block10 block9)
    (on-table block3)
    (on-table block12)
    (on-table block11)
    (on-table block2)
    (on block7 block2)
    (on block13 block7)
    (on-table block5)
    (on-table block1)
  ))
)