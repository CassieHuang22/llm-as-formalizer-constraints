(define (problem blocksworld-p57)
;; CONSTRAINT Odd numbered blocks cannot be stacked on even numbered blocks.
  (:domain blocksworld)
  (:objects block1 block2 block3 block4 block5 block6 block7 block8 block9 block10 block11 block12 )
  (:init 
    (on-table block11)
    (clear block11)
    (on-table block10)
    (clear block10)
    (on-table block8)
    (clear block8)
    (on-table block1)
    (clear block1)
    (on-table block3)
    (clear block3)
    (on-table block7)
    (clear block7)
    (on-table block9)
    (clear block9)
    (on-table block4)
    (clear block4)
    (on-table block12)
    (clear block12)
    (on-table block6)
    (clear block6)
    (on-table block2)
    (clear block2)
    (on-table block5)
    (clear block5)
    (arm-empty)
;; BEGIN ADD
    (odd block1) (even block2) (odd block3) (even block4) (odd block5) (even block6)
    (odd block7) (even block8) (odd block9) (even block10) (odd block11) (even block12)
;; END ADD
  )
  (:goal (and 
    (on-table block6)
    (on block9 block6)
    (on-table block2)
    (on block3 block2)
    (on block7 block3)
    (on block4 block7)
    (on block11 block4)
    (on block12 block11)
    (on block8 block12)
    (on block1 block8)
    (on block10 block1)
    (on block5 block10)
  ))
)